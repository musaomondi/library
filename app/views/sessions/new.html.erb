<% provide(:title, "Log in") %>
<h1>Log in</h1>
<%= form_for(:session, url: login_path) do |f| %>

    <%= f.label :email %>
    <%= f.email_field :email%>

    <%= f.label :password%>
    <%= link_to "(Forgot password)", "#" %>
    <%= f.password_field :password%>

<%end%>

<%= f.submit "Log in" %>

<p> New user? <%= link_to "Sign up now!", new_user_path %></p>
</div>
</div>
